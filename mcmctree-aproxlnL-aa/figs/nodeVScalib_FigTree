begin trees;
	tree tree_1 = [&R] ((((1_human:1.0,(2_chimpanzee:1.0,3_bonobo:1.0)[&label=12]:1.0)[&label=11,!color=#0000cc]:1.0,4_gorilla:1.0)[&label=10]:1.0,(5_orangutan:1.0,6_sumatran:1.0)[&label=13]:1.0)[&label=9,!color=#0000cc]:1.0,7_gibbon:1.0)[&label=8,!color=#0000cc];
	tree tree_2 = [&R] ((((human:1.0,(chimpanzee:1.0,bonobo:1.0):1.0)[&label="B(.06,.08)",!color=#0000cc]:1.0,gorilla:1.0):1.0,(orangutan:1.0,sumatran:1.0):1.0)[&label="B(.12,.16)",!color=#0000cc]:1.0,gibbon:1.0)[&label="U(1.0)",!color=#0000cc];
